        <DataSourceName>MainSourceDW</DataSourceName>
        <CommandText>SELECT        DimCustomer.DimCustomer_ID, DimCustomer.Customer_Name, StagingCustomer.AvgClearBalance, StagingCustomer.AvgUnclearBalance, StagingCustomer.AvgTransaction_amount
